{
  "name": "torqued-automation-bundle",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "shopify:deploy": "node scripts/build-shopify.mjs && node scripts/upload-to-shopify.mjs && node scripts/create-section.mjs",
    "test": "vitest",
    "test:playwright": "playwright test",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "migrate-secrets": "node scripts/migrate-secrets.js --provider=aws",
    "optimize:bundle": "node scripts/optimize-bundle.mjs",
    "deploy:edge": "node scripts/deploy-edge.mjs",
    "analyze:bundle": "npx vite-bundle-analyzer",
    "performance:audit": "lighthouse http://localhost:4173 --output=json --output-path=./reports/lighthouse.json",
    "security:scan": "npm audit --audit-level moderate",
    "ai:optimize": "node scripts/ai-optimize.mjs"
  },
  "dependencies": {
    "express": "4.18.2",
    "ioredis": "5.3.2",
    "node-fetch": "3.3.0",
    "passport": "0.6.0",
    "passport-oauth2": "1.6.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-joyride": "2.5.0",
    "react-window": "1.8.7",
    "@tanstack/react-query": "4.35.0",
    "zustand": "4.4.0",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "4.3.1",
    "@playwright/test": "1.40.1",
    "esbuild": "0.18.16",
    "eslint": "8.45.0",
    "lighthouse": "10.4.0",
    "prettier": "2.8.8",
    "typescript": "5.6.3",
    "vite": "5.3.0",
    "vitest": "1.6.0",
    "vite-bundle-analyzer": "0.7.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ksksrbiz-arch/TOrqued"
  }
}
